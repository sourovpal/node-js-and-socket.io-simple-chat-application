<%- include('../layouts/header', {
    includes:['styles'],
}); %>




<div class="sign-in-page">
    <div class="inner">
        <div class="container">
            <div class="row">
                <div class="col-lg-5 col-md-8 col-sm-10 mx-auto">
                    <div class="box">
                        <div class="box-body">
                            <div>
                                <h3 class="form-title">Sign In</h3>
                            </div>
                            <div>
                                <form id="signInForm" class="sign-in-form" action="">
                                    <div class="mb-3">
                                        <input id="email" type="text" class="form-input" placeholder="Email Address">
                                    </div>
                                    <div class="mb-3">
                                        <input id="password" type="password" class="form-input" placeholder="Password" value="111111">
                                    </div>
                                    <div class="">
                                        <button class="sign-in-btn">Sign In </button>
                                    </div>
                                </form>
                            </div>
                            <div class="d-flex justify-content-between mt-2 mx-4">
                                <a class="text-decoration-none fs-14 text-center d-inline" href="/user/sign-up">Create Account.</a>
                                <a class="text-decoration-none fs-14 text-center d-inline" href="/forgot-password">Forgot your password ?</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






<%- include('../layouts/footer', {
    scripts:['/js/auth.script.min.js'],
}); %>
