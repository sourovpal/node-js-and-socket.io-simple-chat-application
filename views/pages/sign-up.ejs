<%- include('../layouts/header', {
    includes:['styles'],
}); %>
<!--  -->
<div class="sign-up-page">
    <div class="inner">
        <div class="container">
            <div class="row">
                <div class="col-lg-5 col-md-8 col-sm-10 mx-auto">
                    <div class="box">
                        <div class="box-body">
                            <div>
                                <h3 class="form-title">Sign Up</h3>
                            </div>
                            <div>
                                <form id="signUpForm" class="sign-up-form" action="">
                                    <div class="d-flex mb-3">
                                        <div class="pe-1">
                                            <input id="fname" type="text" class="form-input" placeholder="First Name">
                                        </div>
                                        <div class="ps-1">
                                            <input id="lname" type="text" class="form-input" placeholder="Last Name">
                                        </div>
                                    </div>
                                    <div class="mb-3">
                                        <input id="email" type="text" class="form-input" placeholder="Email Address">
                                    </div>
                                    <div class="mb-3">
                                        <input id="password" type="password" class="form-input" placeholder="Password">
                                    </div>
                                    <div class="mb-3">
                                        <input id="confirmPassword" type="password" class="form-input" placeholder="Confirm Password">
                                    </div>
                                    <div>
                                        <button class="sign-up-btn">Sign Up</button>
                                    </div>
                                </form>
                            </div>
                            <div>
                                <a class="text-decoration-none fs-14 text-center d-block mt-2 mb-0" href="/sign-in">Already have an account ? Login here.</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--  -->
<%- include('../layouts/footer', {
    scripts:['/js/auth.script.min.js'],
}); %>
